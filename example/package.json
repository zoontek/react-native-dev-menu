    "start": "react-native start",
    "reinstall": "yarn clean && yarn install"
  },
  "dependencies": {
    "react": "18.0.0",
    "react-native": "0.69.0",
    "react-native-dev-menu": "../"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/runtime": "^7.18.3",
    "metro-react-native-babel-preset": "^0.70.3"
  }
}
